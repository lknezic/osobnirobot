{
  "_note": "This is a reference config only. The entrypoint.sh generates the actual config from environment variables at container startup.",
  "gateway": {
    "port": 18789,
    "bind": "lan",
    "auth": {
      "mode": "token",
      "token": "SET_VIA_GATEWAY_TOKEN_ENV_VAR"
    },
    "controlUi": {
      "enabled": true,
      "allowInsecureAuth": true
    },
    "trustedProxies": ["172.17.0.0/16", "127.0.0.1", "0.0.0.0/0"]
  },
  "models": {
    "providers": {
      "google": {
        "apiKey": "SET_VIA_GOOGLE_AI_KEY_ENV_VAR",
        "baseUrl": "https://generativelanguage.googleapis.com/v1beta",
        "models": [{"id": "gemini-2.0-flash", "name": "Gemini 2.0 Flash"}]
      },
      "anthropic": {
        "apiKey": "SET_VIA_ANTHROPIC_API_KEY_ENV_VAR",
        "baseUrl": "https://api.anthropic.com",
        "models": [{"id": "claude-sonnet-4-20250514", "name": "Claude Sonnet 4"}]
      }
    }
  },
  "agents": {
    "defaults": {
      "workspace": "/home/node/.openclaw/workspace",
      "model": {
        "primary": "google/gemini-2.0-flash"
      }
    }
  },
  "messages": {
    "responsePrefix": "auto"
  }
}
