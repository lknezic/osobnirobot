# Orchestrator config
ORCHESTRATOR_PORT=3500
ORCHESTRATOR_SECRET=your-secret-here

# Docker
DOCKER_SOCKET=/var/run/docker.sock
CONTAINER_IMAGE=instantworker/worker:latest

# AI API Keys (injected into containers)
GOOGLE_AI_KEY=your-google-ai-key
ANTHROPIC_API_KEY=your-anthropic-key

# Worker templates path (absolute path on host)
WORKER_TEMPLATES_PATH=/opt/instantworker/worker-templates

# Port ranges for containers (expanded for multi-employee)
GATEWAY_PORT_START=20000
GATEWAY_PORT_END=21999
NOVNC_PORT_START=22000
NOVNC_PORT_END=23999
